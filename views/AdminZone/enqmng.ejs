        <div class="row" style="min-height:520px;">
            <div class="col-md-2"  style="font: size 10px"><br>
               <center> <span class="fa fa-user-circle-o" style="font-size:67px;"></span><br><b>Admin</b></center>
                    <span class="fa fa-dashboard">&nbsp;<b style="font-size: 0.9rem;"><a class="dashline" href="/dashboard">Dashboard</a></b></span> <br><br>
                    <span class="fa fa-question-circle">&nbsp;<b style="font-size: 0.9rem;"><a class="dashline" href="/enqmng">Enquiry Management</a></b></span><br><br>
                    <span class="fa fa-user">&nbsp;<b style="font-size: 0.9rem;"><a class="dashline" href="/usermng">User Management</a></b></span><br><br>
                    <span class="fa fa-plus">&nbsp;<b style="font-size: 0.9rem;"><a class="dashline" href="/aplyusermng">Apply User Management</a></b></span><br><br>
                    <span class="fa fa-plus">&nbsp;<b style="font-size: 0.9rem;"><a class="dashline" href="/addjob">Add Job</a></b></span><br><br>
                   
                    <span class=" fa fa-solid fa-briefcase">&nbsp;<b style="font-size: 0.9rem;"><a class="dashline" href="/jobmng">Job Management</a></b></span><br><br>
                    <span class="fa fa-key">&nbsp;<b style="font-size: 0.9rem;"><a class="dashline" href="/password">Change Password</a></b></span><br><br>
                    <span class="fa fa-sign-out">&nbsp;<b style="font-size: 0.9rem;">
                        <a class="dashline" href="/logout">Logout</a>
                        </b></span>                  

            </div>
           <div class="col-md-1"></div>
            <div class="col-md-9"><br>
                    <center><h1> <% title %> </h1></center>
                    <table class="table table-responsive" style="background:white;padding: 1%;">
                        <tr>
                            <th>Sr.No</th>
                            <th>First Name</th>
                            <th>Last Name</th>
                            <th>Email</th>
                            <th>Mobile Number</th>
                            <th>Message </th>
                        </tr>
                        <% for(var i=0;i<data.length;i++) 
                        {
                        %>
                       <tr>
                        <td> <%= (i+1) %> </td>
                        <td> <%= data[i].f_name %> </td>
                        <td> <%= data[i].l_name %> </td>
                        <td> <%= data[i].email %> </td>
                        <td> <%= data[i].mob %> </td>
                        <td> <%= data[i].msg %> </td>
                       </tr>
                        <% } %>
                    </table>
           </div>
        </div>
    
    </div>
    <div class="container-fluid">
    <div class="row">
      <div class="col-md-12" style="min-height: 50px;color: white; background-color: black;">
